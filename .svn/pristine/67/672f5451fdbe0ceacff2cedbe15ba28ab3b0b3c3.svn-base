				 var phone=$(".Factory-url-name-show").find('ul').eq(i).find('input').eq(1).val();
				 var email=$(".Factory-url-name-show").find('ul').eq(i).find('input').eq(2).val();
				 var type=$(".Factory-url-name-show").eq(i).find('.factory-audo2').next().val();
				 var auditFileName=$(".Factory-url-name-show").eq(i).find('.factory-audo2').next().next().val();
				 var price=$(".Factory-url-name-show").eq(i).find('b:last').text();
				 infoStr=province+'&&'+address+'&&'+phone+'&&'+email+'&&'+type+'&&'+auditFileName+'&&'+price+',';
				 addressFromData+=infoStr;	
			 });
			 $("#addressFromData").val(addressFromData); 
		 }else{
			 var urlFormData='';

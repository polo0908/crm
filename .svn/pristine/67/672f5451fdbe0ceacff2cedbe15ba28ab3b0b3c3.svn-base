				 var url=$(".Factory-url-list-show").eq(i).find('input:first').val();
				 var type=$(".Factory-url-list-show").eq(i).find('.factory-audo2').next().val();
				 var auditFileName=$(".Factory-url-list-show").eq(i).find('.factory-audo2').next().next().val();
				 var price=$(".Factory-url-list-show").eq(i).find('b:last').text();
				 infoStr=url+'&&'+type+'&&'+auditFileName+'&&'+price+',';
				 urlFormData+=infoStr;	
			 });
			 $("#urlFormData").val(urlFormData); 
		 }
		 $("#file_upload_id").ajaxSubmit({
			type : "post",
			url : "${ctx}/audit/addAuditToPay.do",
			dataType : "text",
			success : function(result) {
				window.location.href = "${ctx}/audit/orderDetailPay.do?orderId="+result;
			},
			error : function() {
				easyDialog.open({
					container : {
						content : 'Add Failed'
					},
					autoClose : 1000
			  });
			}
		});
	 }

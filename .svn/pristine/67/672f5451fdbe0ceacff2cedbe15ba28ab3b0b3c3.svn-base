	//填写工厂url计算价格(只需要类型)
	function calculateUrlPrice(){
		 var urlDataStr='';
		 $(".Factory-url-list-show").each(function (i) {
			 var num=(i+1);
			 var type= $(".Factory-url-list-show").eq(i).find('.factory-audo2').next().val();
			 infoStr=num+'&'+type+',';
			 urlDataStr+=infoStr;	
		 });
		 $("#urlDataStr").val(urlDataStr);
		 $("#file_upload_id").ajaxSubmit({
			type : "post",
			url : "${ctx}/audit/calculateUrlPrice.do",
			dataType : "text",
			success : function(result) {
				var json = eval("(" + result +")");	
				$('.Factory-url-list-show').each(function(i){
				    $(this).find('b:last').text(json.price[i]);
				})
				$("#showTotalPrice").text(json.totalPrice);
			}, 
			error : function() {
				easyDialog.open({
					container : {
						content : 'Add Failed'
					},
					autoClose : 1000
			  });
			}
		});
	}  
	

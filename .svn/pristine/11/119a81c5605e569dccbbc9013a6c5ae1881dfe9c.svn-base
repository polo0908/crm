body {
	margin: 0;
	padding: 0;
	color: #333;
	font: 14px/1.8 Tahoma, Geneva, '\5B8B\4F53';
}
ol {
	list-style: none outside none;
	margin: 0;
	padding: 0;
}
.w-ui-step-cont-text{color: #666;}
.ui-step {
	z-index: 999;
	padding-left: 12px;
	color: #fff;
	margin: 0;
	/*padding:60px;*/
/*	margin-bottom: 35px;*/
	position: relative;
}
.w-last-text{color: #666;}
.ui-step:after {
	display: block;
	content: "";
	height: 0;
	font-size: 0;
	clear: both;
	overflow: hidden;
	visibility: hidden;
}
.ui-step li {
	float: left;
	position: relative;
}
.ui-step .step-end {
	width: 120px;
	position: absolute;
	/*top:60px;*/
	right: -100px;
}
.ui-step-line {
	margin-left: 30px;
	margin-right: 20px;
	color: #fff;
	margin-top: 60px;
	position: relative;
	height:10px;
	background-color: #e0e0e0;
	box-shadow: inset 0 1px 1px rgba(0,0,0,.2);
}

.ui-step-cont .w-last-text{
	top: -30px;
	position: relative;
	left: -42px;
	font-size: 14px;
}

.ui-step-line2 {
	margin-left: 10px;
	color: #fff;
	height:10px;
	background-color:#28b779;
	box-shadow: inset 0 1px 1px rgba(0,0,0,.2);
	position: absolute;
	width: 40%;
	margin-top: 0px;
	z-index: 99;
}
.step-end .ui-step-line { display: none; }
.ui-step-cont {
	width: 120px;
	position: absolute;
	top:-25px;
	left: -15px;
	text-align: center;
}
.ui-step-cont-number {
	display: inline-block;
*zoom:1;
	position: absolute;
	left: 10px;
	top: 0;
	width: 47px;
	height: 30px;
	line-height: 30px;
	background: url(img/ui-step_cover_30x30.png) center no-repeat\9;
	border-radius: 50%;
	border-left: 2px solid #fff;
	border-right: 2px solid #fff;
	border: 2px solid rgba(224,224,224,1);
	font-family: tahoma;
	font-weight: bold;
	font-size: 16px;
	background-color: #b9b9b9;
	box-shadow: inset 1px 1px 2px rgba(0,0,0,.2);
}
.ui-step-cont-text {
	position: absolute;
    top: 31px;
    left: 17px;
    font-size: 14px;
    border: 1px solid #eee;
    width: 27px;
    padding: 3px 1px;
/*     word-break: break-all; */
/*     display: -webkit-box; */
/*     -webkit-box-orient: vertical; */
/*     -webkit-line-clamp: 1; */
    overflow: hidden;
	white-space: nowrap;
	height: 14px;
	text-overflow: ellipsis;
}
.w-div-img{
	width: 80px;
    top: -114px;
    position: absolute;
}
/** 姝ラ鏁板畾涔� **/
.ui-step-3 li { width: 50%; }
.ui-step-4 li { width: 33.3%; }
.ui-step-5 li { width: 25%; }
.ui-step-6 li { width: 20%; }
/** The default style (榛樿椋庢牸) **/
/* Done */
.step-done .ui-step-cont-number { background-color:#28b779; }
.step-done .ui-step-cont-text { color:#28b779; }
.step-done .ui-step-line { background-color:#28b779; }
/* Active */
.step-active .ui-step-cont-number { background-color:#28b779; }
.step-active .ui-step-cont-text {
	color:#28b779;
	font-weight: bold;
}
.step-active .ui-step-line { background-color: #e0e0e0; }


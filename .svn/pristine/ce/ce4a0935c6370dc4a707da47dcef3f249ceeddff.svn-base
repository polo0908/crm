tr:hover {background: transparent;}
.w-font{width: 100%;float: left;}
.w-font h2 {
    color: #555555;
    font-size: 24px;
    font-weight: normal;
    float: none;
    text-shadow: 0 1px 0 #ffffff;
    margin-left: 20px;
    position: relative;
}
.container-fluid hr{margin:0;}
.container-fluid .w-row{padding: 20px 0;background: #fff;}
.container-fluid .w-row .span12{padding: 0 20px;width:80%;}
.delitabl {float: left;border: 0;position: relative;}
.dot-right {
    font-size: 0;
    line-height: 0;
    border-width: 10px;
    border-color: #029a00;
    border-right-width: 0;
    border-style: dashed;
    border-left-style: solid;
    border-top-color: transparent;
    border-bottom-color: transparent;
}
.dot-right2 {
    font-size: 0;
    line-height: 0;
    border-width: 10px;
    border-color: #28b779;
    border-right-width: 0;
    border-style: dashed;
    border-left-style: solid;
    border-top-color: transparent;
    border-bottom-color: transparent;
    bottom: -4px;
}
.dot-right-red {
    font-size: 0;
    line-height: 0;
    border-width: 14px;
    border-color: #f74d4d;
    border-right-width: 0;
    border-style: dashed;
    border-left-style: solid;
    border-top-color: transparent;
    border-bottom-color: transparent;
}

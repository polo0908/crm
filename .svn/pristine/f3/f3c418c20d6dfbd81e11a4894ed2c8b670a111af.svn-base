});

//右箭头点击处理
$("#next").click(function(){ 
  curIndex = (curIndex < imgLen - 1) ? (++curIndex) : 0;
  $('.indexList').find('li:eq(1)').text(curIndex + 1 + "/" + imgLen);
  changeTo(curIndex);
});


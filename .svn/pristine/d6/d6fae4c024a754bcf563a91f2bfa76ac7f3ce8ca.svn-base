	float: left;
}

.ribbon {
	display: block;
	width: 50px;
	position: absolute;
	color: #fff;
	padding: 10px 0 0px;
	text-align: center;
	font-family: arial, sans-serif;
	font-weight: bold;
	text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.3);
}

.ribbon:before {
	display: block;
	width: 50px;
	height: 0;
	position: absolute;
	top: 4px;
	content: "";
	border-top: 1px dotted rgba(0, 0, 0, 0.2);
	border-bottom: 1px dotted rgba(255, 255, 255, 0.3);
}

.ribbon:after {
	display: block;
	width: 0;
	height: 0;
	content: "";
	position: relative;
	bottom: -15px;
	z-index: 100;
	border-width: 0 25px 15px 25px;
	border-style: solid;
}

.ribbon.dark-blue {
	background-color: #202a52;
}

.ribbon.dark-blue:after {
	border-color: #202a52 #202a52 transparent #202a52;
}


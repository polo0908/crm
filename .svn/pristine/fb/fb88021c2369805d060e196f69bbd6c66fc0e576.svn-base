<c:set var="ctx" value="${pageContext.request.contextPath}" />
<%List<Object> emailReceive = (List<Object>)request.getAttribute("emailReceive");%>
<%List<Object> emailSend = (List<Object>)request.getAttribute("emailSend");%>


<!DOCTYPE html>
<html lang="en">
<head>
<title>ImportX</title>
